{
  "name": "react-cell-grid",
  "version": "1.1.0",
  "description": "A grid react component",
  "main": "dist/Grid.js",
  "module": "dist/Grid.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anthony-dallagnola/react-grid.git"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "start:simple": "parcel './examples/src/simple/index.html' -p 4001",
    "start:multiple": "parcel './examples/src/multiple headers/index.html' -p 4001",
    "start:sort": "parcel './examples/src/sort/index.html' -p 4001",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collect-coverage",
    "lint": "eslint src/**/*.tsx",
    "build": "rm -rf dist && npx webpack --mode production",
    "build:dev": "rm -rf dist && npx webpack --mode development",
    "abuild": "rm -rf dist && webpack -p --mode production",
    "/build": "rm -rf dist && NODE_ENV=production babel src/lib --extensions '.tsx' --out-dir dist --copy-files",
    "//build": "rm -rf dist && NODE_ENV=production babel src/lib --extensions '.tsx' --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "check-types": "tsc",
    "transpile": "rm -rf dist && babel src/lib -d dist --copy-files --ignore *.tsx,*.scss,*.d.ts,*.ts",
    "//": "Called before npm publish",
    "//prepublishOnly": "npm run transpile",
    "publish:dry-run": "npm publish --dry-run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "keywords": [
    "react",
    "grid",
    "component",
    "cell",
    "react-grid",
    "react-component",
    "simple-react-grid"
  ],
  "author": {
    "name": "Anthony DALL'AGNOLA-BOMIER",
    "email": "anthony.dallagnola@gmail.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.4.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.3",
    "sass-jest": "^0.1.7",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "styled-components": "^5.1.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
